s1 = {10, 20, 30}
s1.add(40)
print("s1 after s1.add(40):\t", s1)
s2 = s1.copy()
print("s2:\t", s2)
print("id(s1):\t", id(s1))
print("id(s2):\t", id(s2))
# s1.remove(100)  # KeyError
s1.discard(10)  # doesn't raise an error
print("s1 after s1.discard(10):\t", s1)
s1.pop()
print("s1 after s1.pop():\t", s1)

s3 = set()  # s3 is empty set
s1.pop()
print("s2 after s3.pop():\t", s3)
