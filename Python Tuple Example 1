t1 = ()
print("1 Empty tuple:\t", t1, type(t1))
t2 = tuple()
print("2 Empty tuple:\t", t1, type(t2))
t3 = (100)
print("3 tuple with single element:\t", t1, type(t3))
t4 = (100,)
print("4 tuple with single element:\t", t1, type(t4))
t5 = tuple([100])
print("5 tuple with single element:\t", t1, type(t5))
